    %load/vec4 v0x134679860_0;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_19.2, 4;
    %vpi_call/w 3 542 "$display", "\012*** ALL TESTS PASSED ***" {0 0 0};

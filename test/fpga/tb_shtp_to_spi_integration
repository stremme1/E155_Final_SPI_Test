v0x1346779c0_0 .var/i "i", 31 0;
v0x134677a80_0 .var/i "j", 31 0;
TD_tb_shtp_to_spi_integration.mcu_read_packet ;
    %vpi_call/w 3 152 "$display", "[%0t] MCU: Starting SPI transaction - CS goes low", $time {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1346796b0_0, 0, 1;
    %delay 1665000, 0;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x1346779c0_0, 0, 32;
T_1.2 ;
    %load/vec4 v0x1346779c0_0;
    %cmpi/s 16, 0, 32;
    %jmp/0xz T_1.3, 5;
    %pushi/vec4 0, 0, 8;
    %ix/getv/s 4, v0x1346779c0_0;
    %store/vec4a v0x13467a860, 4, 0;
    %pushi/vec4 7, 0, 32;
    %store/vec4 v0x134677a80_0, 0, 32;
T_1.4 ;
    %load/vec4 v0x134677a80_0;
    %cmpi/s 0, 0, 32;
    %flag_inv 5; GE is !LT
    %jmp/0xz T_1.5, 5;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x13467a900_0, 0, 1;
    %delay 10000, 0;
    %load/vec4 v0x13467aa60_0;
    %ix/getv/s 4, v0x1346779c0_0;
    %flag_mov 8, 4;
    %ix/getv/s 5, v0x134677a80_0;
    %flag_or 4, 8;
    %store/vec4a v0x13467a860, 4, 5;
    %delay 490000, 0;
    %load/vec4 v0x134677a80_0;
    %cmpi/s 0, 0, 32;
    %flag_or 5, 4; GT is !LE
    %flag_inv 5;
    %jmp/1 T_1.8, 5;
    %flag_mov 8, 5;
    %load/vec4 v0x1346779c0_0;
    %cmpi/s 15, 0, 32;
    %flag_or 5, 8;
T_1.8;
    %jmp/0xz  T_1.6, 5;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x13467a900_0, 0, 1;
    %delay 500000, 0;
T_1.6 ;
    %load/vec4 v0x134677a80_0;
    %subi 1, 0, 32;
    %store/vec4 v0x134677a80_0, 0, 32;
    %jmp T_1.4;
T_1.5 ;
    %vpi_call/w 3 178 "$display", "[%0t] MCU: Byte[%0d] = 0x%02X", $time, v0x1346779c0_0, &A<v0x13467a860, v0x1346779c0_0 > {0 0 0};
    %load/vec4 v0x1346779c0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x1346779c0_0, 0, 32;
    %jmp T_1.2;
T_1.3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x13467a900_0, 0, 1;
    %delay 1000000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x1346796b0_0, 0, 1;
    %delay 1665000, 0;
    %vpi_call/w 3 187 "$display", "[%0t] MCU: Packet complete - CS high", $time {0 0 0};
    %end;

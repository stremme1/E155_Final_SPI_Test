    %load/vec4a v0x13467a860, 4;
    %pushi/vec4 11, 0, 8;
    %cmp/e;
    %flag_get/vec4 4;
    %store/vec4 v0x134673b60_0, 0, 1;
    %fork TD_tb_shtp_to_spi_integration.check_test, S_0x134673990;
    %join;
    %pushi/str "TC8: Quat Y LSB correct";
    %store/str v0x134673bf0_0;
    %ix/load 4, 6, 0;
    %flag_set/imm 4, 0;
    %load/vec4a v0x13467a860, 4;
    %pushi/vec4 184, 0, 8;
    %cmp/e;
    %flag_get/vec4 4;
    %store/vec4 v0x134673b60_0, 0, 1;
    %fork TD_tb_shtp_to_spi_integration.check_test, S_0x134673990;
    %join;
    %delay 3330000, 0;
    %vpi_call/w 3 529 "$display", "\000" {0 0 0};
    %vpi_call/w 3 534 "$display", "\012========================================" {0 0 0};
    %vpi_call/w 3 535 "$display", "Test Summary" {0 0 0};

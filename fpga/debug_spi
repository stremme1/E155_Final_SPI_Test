T_3 ;
    %wait E_0x14a9044b0;
    %load/vec4 v0x14a914640_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_3.0, 8;
    %pushi/vec4 0, 0, 4;
    %load/vec4 v0x14a9145a0_0;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x14a914780_0, 0;

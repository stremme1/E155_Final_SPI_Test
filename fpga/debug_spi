    %concat/vec4; draw_concat_vec4
    %pushi/vec4 0, 0, 4;
    %load/vec4 v0x14a9145a0_0;
    %concat/vec4; draw_concat_vec4
    %vpi_call/w 3 15 "$display", "[%0t] LATCHED: drum_code=%0d, tx_buffer=0x%02X, tx_shift_reg=0x%02X", $time, v0x14a9145a0_0, S<1,vec4,u8>, S<0,vec4,u8> {2 0 0};
T_3.0 ;

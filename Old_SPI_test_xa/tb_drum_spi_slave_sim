TD_tb_drum_spi_slave.mcu_read_command ;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x15491b4e0_0, 0, 8;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x15491b570_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x15491ae70_0;
    %nor/r;
    %flag_set/vec4 9;
    %flag_get/vec4 9;
    %jmp/0 T_0.2, 9;
    %load/vec4 v0x15491b570_0;
    %cmpi/s 10000, 0, 32;
    %flag_get/vec4 5;
    %and;
T_0.2;
    %flag_set/vec4 8;
    %jmp/0xz T_0.1, 8;
    %delay 333000, 0;
    %load/vec4 v0x15491b570_0;
    %addi 1, 0, 32;
    %store/vec4 v0x15491b570_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %load/vec4 v0x15491ae70_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.3, 8;
    %vpi_call/w 3 108 "$display", "[%0t] ERROR: DONE signal never asserted! done=%b", $time, v0x15491ae70_0 {0 0 0};
    %pushi/vec4 255, 0, 8;
    %store/vec4 v0x15491a190_0, 0, 8;
    %jmp T_0.4;
T_0.3 ;
    %vpi_call/w 3 111 "$display", "[%0t] MCU: DONE signal detected, starting SPI read", $time {0 0 0};
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x15491b4e0_0, 0, 8;
    %delay 666000, 0;
    %pushi/vec4 7, 0, 32;
    %store/vec4 v0x15491b040_0, 0, 32;
T_0.5 ;
    %load/vec4 v0x15491b040_0;
    %cmpi/s 0, 0, 32;
    %flag_inv 5; GE is !LT
    %jmp/0xz T_0.6, 5;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x15491b210_0, 0, 1;
    %delay 50000, 0;
    %load/vec4 v0x15491b450_0;
    %ix/getv/s 4, v0x15491b040_0;
    %store/vec4 v0x15491b4e0_0, 4, 1;
    %delay 1000000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x15491b210_0, 0, 1;
    %delay 500000, 0;
    %vpi_call/w 3 130 "$display", "[%0t] MCU: Bit %0d = %b", $time, v0x15491b040_0, v0x15491b450_0 {0 0 0};
    %load/vec4 v0x15491b040_0;
    %subi 1, 0, 32;
    %store/vec4 v0x15491b040_0, 0, 32;
    %jmp T_0.5;
T_0.6 ;
    %load/vec4 v0x15491b4e0_0;
    %store/vec4 v0x15491a190_0, 0, 8;
    %vpi_call/w 3 134 "$display", "[%0t] MCU: Received command: 0x%02X (%0d)", $time, v0x15491a190_0, v0x15491a190_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x15491b0d0_0, 0, 1;
    %delay 3330000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x15491b0d0_0, 0, 1;
    %delay 3330000, 0;
T_0.4 ;
    %end;

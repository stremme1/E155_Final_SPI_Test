    %vpi_call/w 3 208 "$error", "Test 5a failed: Expected 0x06 (latest trigger), got 0x%02X", v0x15491ab50_0 {0 0 0};
T_10.11 ;
    %vpi_call/w 3 209 "$display", "Test 5a PASSED: Latest rapid trigger (code 6) received correctly\012" {0 0 0};
    %delay 3330000, 0;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0x15491a620_0, 0, 4;
    %fork TD_tb_drum_spi_slave.trigger_drum, S_0x15491a460;
    %join;
    %fork TD_tb_drum_spi_slave.mcu_read_command, S_0x15491a020;
    %join;
    %load/vec4 v0x15491a190_0;
    %store/vec4 v0x15491ac00_0, 0, 8;
    %load/vec4 v0x15491ac00_0;
    %cmpi/e 2, 0, 8;
    %jmp/0xz  T_10.12, 4;
    %jmp T_10.13;
T_10.12 ;
    %vpi_call/w 3 216 "$error", "Test 5b failed: Expected 0x02, got 0x%02X", v0x15491ac00_0 {0 0 0};
T_10.13 ;
    %vpi_call/w 3 217 "$display", "Test 5b PASSED: Second trigger after acknowledgment received correctly\012" {0 0 0};
    %delay 3330000, 0;
    %pushi/vec4 4, 0, 4;
    %store/vec4 v0x15491a620_0, 0, 4;
    %fork TD_tb_drum_spi_slave.trigger_drum, S_0x15491a460;
    %join;
    %fork TD_tb_drum_spi_slave.mcu_read_command, S_0x15491a020;
    %join;
    %load/vec4 v0x15491a190_0;
    %store/vec4 v0x15491acb0_0, 0, 8;
    %load/vec4 v0x15491acb0_0;
    %cmpi/e 4, 0, 8;
    %jmp/0xz  T_10.14, 4;
    %jmp T_10.15;
T_10.14 ;
    %vpi_call/w 3 224 "$error", "Test 5c failed: Expected 0x04, got 0x%02X", v0x15491acb0_0 {0 0 0};
T_10.15 ;
    %vpi_call/w 3 225 "$display", "Test 5c PASSED: Third trigger after acknowledgment received correctly\012" {0 0 0};
    %vpi_call/w 3 227 "$display", "\012=== All Tests PASSED ===" {0 0 0};
    %vpi_call/w 3 228 "$display", "Time: %0t", $time {0 0 0};
    %delay 33300000, 0;
    %vpi_call/w 3 230 "$finish" {0 0 0};
    %end;
    .thread T_10;
